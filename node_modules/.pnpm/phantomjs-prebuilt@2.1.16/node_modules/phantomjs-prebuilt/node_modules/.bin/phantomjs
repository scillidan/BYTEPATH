#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/git/BYTEPATH-blogs/node_modules/.pnpm/phantomjs-prebuilt@2.1.16/node_modules/phantomjs-prebuilt/bin/node_modules:/mnt/d/git/BYTEPATH-blogs/node_modules/.pnpm/phantomjs-prebuilt@2.1.16/node_modules/phantomjs-prebuilt/node_modules:/mnt/d/git/BYTEPATH-blogs/node_modules/.pnpm/phantomjs-prebuilt@2.1.16/node_modules:/mnt/d/git/BYTEPATH-blogs/node_modules/.pnpm/node_modules:/mnt/d/git/BYTEPATH-blogs/node_modules:/mnt/d/git/node_modules:/mnt/d/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/git/BYTEPATH-blogs/node_modules/.pnpm/phantomjs-prebuilt@2.1.16/node_modules/phantomjs-prebuilt/bin/node_modules:/mnt/d/git/BYTEPATH-blogs/node_modules/.pnpm/phantomjs-prebuilt@2.1.16/node_modules/phantomjs-prebuilt/node_modules:/mnt/d/git/BYTEPATH-blogs/node_modules/.pnpm/phantomjs-prebuilt@2.1.16/node_modules:/mnt/d/git/BYTEPATH-blogs/node_modules/.pnpm/node_modules:/mnt/d/git/BYTEPATH-blogs/node_modules:/mnt/d/git/node_modules:/mnt/d/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/phantomjs" "$@"
else
  exec node  "$basedir/../../bin/phantomjs" "$@"
fi
